{
  "name": "automentor-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "npm run db:migrate && tsx server/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@paypal/paypal-server-sdk": "latest",
    "@types/express": "latest",
    "@types/express-session": "latest",
    "@types/multer": "latest",
    "@types/nodemailer": "latest",
    "bcryptjs": "latest",
    "dotenv": "latest",
    "drizzle-kit": "latest",
    "drizzle-orm": "latest",
    "drizzle-zod": "latest",
    "express": "latest",
    "express-rate-limit": "latest",
    "express-session": "latest",
    "helmet": "latest",
    "json2csv": "latest",
    "memorystore": "latest",
    "multer": "latest",
    "nodemailer": "latest",
    "postgres": "latest",
    "stripe": "latest",
    "tsx": "latest",
    "typescript": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@types/node": "^24.7.0"
  }
}
